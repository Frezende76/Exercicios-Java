
public class TestaLacos {

	public static void main(String[] args) {
		for(int multip = 1; multip <= 10; multip++) {
			for (int cont = 1; cont <= 10; cont++) {
				System.out.println(multip + " X " + cont + " = " +  multip * cont);
				System.out.print(" ");
			}
			System.out.println();
		}
	}
}
